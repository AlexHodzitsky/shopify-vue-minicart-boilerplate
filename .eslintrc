{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"extends": "eslint:recommended",
	"rules": {
		"semi": 2,
		"camelcase": 0,
		"comma-dangle": 2,
		"quotes": 0,
		"no-console": 1,
		"no-var": 1,
		"no-prototype-builtins": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-empty": [
			"error",
			{
				"allowEmptyCatch": true
			}
		]
	}
}
